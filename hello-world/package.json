{
  "name": "hello-world",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "frontend": "bunx --bun vite serve ./frontend",
    "backend": "bun --hot run ./backend/src/main.ts",
    "preview": "bunx --bun vite preview",
    "frontend-build": "bunx --bun vite build ./frontend",
    "type-check": "vue-tsc --build --force",
    "lint": "bunx --bun eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write '{frontend,backend}/src/**/*'",
    "test-frontend": "npx vitest -c ./frontend/vite.config.ts",
    "test-backend": "bun test ./backend/__tests__/*"
  },
  "dependencies": {
    "express": "^4.19.2",
    "mongodb": "^6.7.0",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/bun": "^1.1.3",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.7",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "cypress": "^13.12.0",
    "eslint": "^8.57.0",
    "eslint-plugin-cypress": "^3.3.0",
    "eslint-plugin-vue": "^9.23.0",
    "happy-dom": "^14.12.0",
    "npm-run-all2": "^6.1.2",
    "prettier": "^3.2.5",
    "typescript": "~5.4.0",
    "vite": "^5.2.8",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.11"
  }
}
